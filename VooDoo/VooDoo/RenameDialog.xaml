<Window x:Class="VooDoo.RenameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Task List" Height="100" Width="300" Name="RenameWindow" Icon="/VooDoo;component/VooDoo.jpg">
    <Grid>
        <Button Command="{Binding OKRenameCommand}" CommandParameter="{Binding ElementName=RenameWindow}" Content="OK" Height="23" HorizontalAlignment="Left" Margin="112,35,0,0" Name="button1" VerticalAlignment="Top" Width="75" IsDefault="True" />
        <Button  Command="{Binding CancelRenameCommand}" CommandParameter="{Binding ElementName=RenameWindow}" Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,35,12,0" Name="button2" VerticalAlignment="Top" Width="75" IsCancel="True" />
        <TextBox Height="19" Margin="9,10,12,0" Name="textBox1" VerticalAlignment="Top" AcceptsReturn="False" />
    </Grid>
</Window>
